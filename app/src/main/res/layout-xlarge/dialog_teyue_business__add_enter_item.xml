<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="@dimen/vl_px_80"
    android:background="@color/white"
    android:gravity="center">

    <TextView
        android:id="@+id/teyue_busniess_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/vl_px_30"
        android:layout_marginBottom="@dimen/vl_px_16"
        android:gravity="center"
        android:text="银联/网联"
        android:textAppearance="@style/TextStyle.Black.Size32"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="@dimen/vl_px_1094"
        android:layout_height="@dimen/vl_px_2"
        android:background="@color/gray9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/vl_px_30"
        android:layout_marginBottom="@dimen/vl_px_16"
        android:gravity="center"
        android:text=">"
        android:textAppearance="@style/TextStyle.Black.Size46"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />



</android.support.constraint.ConstraintLayout>